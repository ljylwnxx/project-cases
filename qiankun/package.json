{
    "name": "qiankun",
    "version": "1.0.0",
    "description": " qiankun + vue2",
    "main": "index.js",
    "scripts": {
        "install": "npm-run-all --parallel install:*",
        "install:main-app": "cd main-app && npm install",
        "install:sub-app": "cd sub-app && npm install",
        "serve": "npm-run-all --parallel start:*",
        "start:main-app": "cd main-app && npm run serve ",
        "start:sub-app": "cd sub-app && npm run serve",
        "build": "npm-run-all --parallel build:*",
        "build:main-app": "cd main-app && npm run build",
        "build:sub-app": "cd sub-app && npm run build"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
